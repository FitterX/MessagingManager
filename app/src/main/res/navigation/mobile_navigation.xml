<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_upcoming">

    <fragment
        android:id="@+id/navigation_upcoming"
        android:name="com.message.messagingmanager.ui.upcoming.UpcomingFragment"
        android:label="Upcoming Messages"
        tools:layout="@layout/fragment_upcoming" />

    <fragment
        android:id="@+id/navigation_groups"
        android:name="com.message.messagingmanager.ui.groups.GroupsFragment"
        android:label="Groups"
        tools:layout="@layout/fragment_groups" />

    <fragment
        android:id="@+id/navigation_history"
        android:name="com.message.messagingmanager.ui.history.HistoryFragment"
        android:label="History"
        tools:layout="@layout/fragment_history" />
</navigation>